/**app.wxss**/
Page {
  /* background: #1baceb; */
  position: absolute;
  font-size: 36rpx;
  width: 100%;
  height: 100%;
  display: block;
  background: #FAFAFA;
  overflow-y: hidden;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

/**index.wxss**/
.header ,.body{
  padding: 0 10px;
  background: #ffffff;
  font-size: 12px;
  color: #222;
}
.header{
  height: 32px;
  line-height: 32px;
  color:#F76F63;

}
.header-title{
  float:left;
  font-size: 13px;
}
.header-to-view{
  height: 20px;
  line-height: 20px;
  font-size: 12px;
  float:right;
  background: #fff;
  border: 1px solid #F76F63;
  border-radius: 10px;
  margin-top: 6px;
  color:#F76F63;
}
.body-user-info{
  background:#F76F63;
  color:#fff;
  padding:15px 0;
  height: 120px;
}
.body-header{
  text-align: center;
  margin:5px 0;
}
.body-header-text{
  line-height: 22px;
  background: #ddd;
  font-size: 12px;
  color: #222;
  padding: 2px 10px;
  border-radius:10px;
}
.body-container {
  padding: 10px 0;
  height: 100%;
  width: 100%;
  float:left;
}



.body-foolter{
  width: 90%;
  position: absolute;
  bottom: 20px;
}

.body-to-search{
  border-radius:20px;
  width: 120px;
  font-size: 16px;
  line-height:36px;
  height: 36px;
  background: #F76F63;
  color:#fff;
  border: none;
  margin: 0 auto;

}
.userinfo {
  display: flex;
  position: relative;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color:#fff;
  font-size:13px;
}

.usermotto {
  margin-top: 200px;
}




.like-new-list{
  width: 100%;
  display: flex;
  background: #fff;
  border: 1px solid #ddd;
  border-radius:20rpx;
  height: 140rpx;
  justify-content:center;
  align-items: center;
  margin-bottom: 20rpx;
}

.like-new-list-include{
  width: 95%;
  flex-wrap:nowrap;
  justify-content: flex-start;
  height: 110rpx;
  display: flex;
  background: black;
  background: #fff;
  align-items:center;

}

.like-new-icon{
  width: 110rpx;
  height: 110rpx;
  background: #F76F63;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
  border-radius:20rpx;
}

.like-new-icon-min{
    width: 50rpx;
    height: 50rpx;
}
.like-new-icon-max{
    width: 110rpx;
    height: 110rpx;
}
.like-new-content{
  flex-wrap: nowrap;
  width: 110rpx;
  height: 110rpx;
  display:flex;
  flex-grow: 1;
  align-items:center;
  justify-content: space-between;
}
.like-new-content-text{
  display:flex;
  flex-direction: column;
  justify-content:center;

}
.like-new-content-text-title-x{
  font-size: 32rpx;
  padding-left: 10px;
}
.like-new-content-text-content{
  font-size: 26rpx;
  color: #ccc;
  padding-left: 10px;
}
.like-new-content-button{
  width: 150rpx;
  height: 110rpx;
  display:flex;
  flex-wrap: nowrap;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  font-size: 12px;
}
.like-new-content-button-primary{
  border-radius: 30rpx;
  background: #F76F63;
  line-height: 60rpx;
  height: 60rpx;
  width: 150rpx;
  font-size: 12px;
  color: #fff;
  text-align: center;
}
.like-new-content-button-success{
  border-radius: 30rpx;
  background: pink;
  line-height: 60rpx;
  height: 60rpx;
  width: 150rpx;
  font-size: 12px;
  color: #fff;
  text-align: center;

}
.like-new-content-button-already{
  border-radius: 30rpx;
  background: #fff;
  line-height: 60rpx;
  height: 60rpx;
  width: 150rpx;
  color: #F76F63;
  border: 1px solid #F76F63;
  text-align: center;

}



.testinfo{
  position: absolute;
  top: 0;
  height: 400rpx;
  background: #fff;
  font-size: 12px;
  width: 100%;
  left: 0;
  opacity: 0.6;
}
