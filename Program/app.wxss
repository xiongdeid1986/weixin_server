/**app.wxss**/
Page {
  /* background: #1baceb; */
  position: absolute;
  font-size: 36rpx;
  width: 100%;
  height: 100%;
  display: block;
  background: #FAFAFA;
  overflow-y: hidden;
} 
.container { 
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0; 
  box-sizing: border-box;
}

/**index.wxss**/
.header ,.body{
  padding: 0 10px;
  background: #ffffff;
  font-size: 12px;
  color: #222;
} 
.header{
  height: 100rpx;
  line-height: 100rpx;
  color:#F76F63;
  display: flex;
  flex-wrap: nowrap;
  justify-content:space-between;
  align-items: center;

}
.header-icon{
  height: 60rpx;
  width: 60rpx;
}
.header-title{
  float:left;
  font-size: 13px;
}
.header-to-view{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 12px;
  padding: 0 8px;
  float:right;
  background: #fff;
  border: 1px solid #F76F63;
  border-radius: 10px;
  color:#F76F63;
}
.body-user-info-head{
  position: relative;
  width: 100%;
  height: 300rpx;
}
.body-user-info-bg{
  position: absolute;
  left: 0;
  top: 0;
  background:#F76F63;
  width: 100%;
  height: 300rpx;
}

.body-user-info{
  position: absolute;
  left: 0;
  top: 0;
  color:#fff;
  width: 100%;
  height: 120px;
}
.body-header{
  text-align: center;
  margin:5px 0;
}
.body-header-text{
  line-height: 22px;
  background: #ddd;
  font-size: 12px;
  color: #222;
  padding: 2px 10px;
  border-radius:10px;
}
.body-container {
  padding: 10px 0;
  height: 100%;
  width: 100%;
  float:left;
}



.body-foolter{
  width: 90%;
  position: absolute;
  bottom: 20px;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}

.body-to-search{
  border-radius:20px;
  width: 260rpx;
  font-size: 16px;
  line-height:80rpx;
  height: 80rpx;
  background: #F76F63;
  color:#fff;
  border: none;

}
.body-userinfo {
  display: flex;
  flex-direction: column;
  align-items: center; 
}
.userinfo-avatar-bg{
  width: 138rpx;
  height: 138rpx;
  margin-top: 40rpx;
  margin-bottom: 20rpx;
  padding: 8rpx;
  background: #fff;
  border-radius: 50%;
}
.userinfo-avatar {
  border-radius: 50%;
  width: 138rpx;
  height: 138rpx;
}

.userinfo-nickname {
  color:#fff;
  font-size:16px;
}


.like-new-list{
  width: 100%;
  display: flex;
  background: #fff;
  border: 1px solid #ddd;
  border-radius:20rpx;
  height: 140rpx;
  justify-content:center;
  align-items: center;
  margin-bottom: 20rpx;
}

.like-new-list-include{
  width: 95%;
  flex-wrap:nowrap;
  justify-content: flex-start;
  height: 110rpx;
  display: flex;
  background: black;
  background: #fff;
  align-items:center;
}

.like-new-icon{
  width: 110rpx;
  height: 110rpx;
  background: #F76F63;
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
  border-radius:20rpx;
  overflow:hidden;
}

.like-new-icon-min-4{
    width: 50rpx;
    height: 50rpx;
    flex-grow: 0;
    border-radius:5rpx;
    
}
.like-new-icon-min-4-0,.like-new-icon-min-4-1
{
  margin-bottom: 11rpx;
}
.like-new-icon-min-9
{
    width: 33rpx;
    height: 33rpx;
    flex-grow: 0;
    border-radius:5rpx;
}
.like-new-icon-min-9-0,.like-new-icon-min-9-1,
.like-new-icon-min-9-2,.like-new-icon-min-9-3,
.like-new-icon-min-9-4,.like-new-icon-min-9-5
{
  margin-bottom: 6rpx;
}
.like-new-icon-max{
    width: 110rpx;
    height: 110rpx;
    border-radius:20rpx;
}
.like-new-content{
  flex-wrap: nowrap;
  width: 110rpx;
  height: 110rpx;
  display:flex;
  flex-grow: 1;
  align-items:center;
  justify-content: space-between;
}
.like-new-content-text{
  display:flex;
  flex-direction: column;
  justify-content:center;

}
.like-new-content-text-title-x{
  font-size: 32rpx;
  padding-left: 10px;
}
.like-new-content-text-content{
  font-size: 26rpx;
  color: #ccc;
  padding-left: 10px;
}
.like-new-like-button-public{

    line-height: 60rpx;
    height: 60rpx;
    width: 150rpx;
    border-radius: 30rpx;
    font-size: 13px;
}
.like-new-content-button{
  width: 150rpx;
  height: 110rpx;
  display:flex;
  flex-wrap: nowrap;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
}
.like-new-content-button-primary{
  background: #F76F63;
  color: #fff;
  text-align: center;
  display: flex;
  padding: 0 15rpx;
  align-items: center;
  justify-content:space-between;
}
.like-new-content-like-icon{
  width: 60rpx;
  height: 60rpx;
}
.like-new-content-like-text{
  width: 100rpx;
  height: 60rpx;
}
.like-new-content-button-success{
  background: pink;
  color: #fff;
  text-align: center;

}
.like-new-content-button-already{
  background: #fff;
  color: #F76F63;
  border: 1px solid #F76F63;
  text-align: center;

}



.testinfo{
  position: absolute;
  top: 0;
  height: 400rpx;
  background: #fff;
  font-size: 12px;
  width: 100%;
  left: 0;
  opacity: 0.6;
}
